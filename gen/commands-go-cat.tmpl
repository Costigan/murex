package docs

// Summary stores a one line summary of each builtins
var Summary = map[string]string{
{{ if .Documents }}{{ range .Documents }}{{ if .Synonyms }}
	{{ quote (index .Synonyms 0) }}: {{ quote .Summary }},{{ end }}{{ else }}
	{{ quote .ID }}: {{ quote .Summary }},{{ end }}{{ end }}{{ end }}
}

//Synonym is used for builtins that might have more than one internal alias
var Synonym = map[string]string{
{{ if .Documents }}{{ range .Documents }}{{ if .Synonyms }}{{ $ID := quote (index .Synonyms 0) }}{{ range .Synonyms }}
	{{ quote . }}: {{ $ID }},{{ end }}{{ else }}
	{{ quote .ID }}: {{ quote .ID }},{{ end }}{{ end }}{{ end }}
}